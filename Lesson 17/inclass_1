#include <iostream>
#include <stdio.h>
#include <string>
#include "myfunctions.h"
using namespace std;

class Box{
public:
	double length;
	double width;
	double height;
};

int main(){
	Box box1;
	Box box2;
	double volume = 0.0;
	box1.height = 4.0;
	box1.length = 6.0;
	box1.width = 3.0;
	box2.height = 10.0;
	box2.length = 12.0;
	box2.width = 12.0;
	volume = box1.length * box1.width * box1.height;
	cout << "Volume of box1 is " << volume << endl;
	volume = box2.length * box2.width * box2.height;
	cout << "Volume of box2 is " << volume << endl;

	return 0;
}
