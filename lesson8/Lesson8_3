#include <iostream>
using namespace std;

int main(){
    int age;
    cout << "input your age: ";
    cin >> age;
    if (0<=age && age<3){
        cout << "Baby" << endl;
    }
    else if (3<=age && age<7){
        cout << "Child" << endl;
    }
    else if (7<=age && age<14){
        cout << "School" << endl;
    }
    else if (14<=age && age<18){
        cout << "Teenager" << endl;
    }
    else if (age >= 18){
        cout << "Adult" << endl;
    }
    else{
        cout << "not born yet" << endl;
    }
    
    return 0;
}