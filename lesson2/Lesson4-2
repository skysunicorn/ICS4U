#include <iostream>
#include <math.h>

using namespace std;

#define PI 3.14159265

int main(){
    double param, result;
    param = 60.0;
    result = cos(param * PI / 180.0);
    printf("The cosin of %f degrees is %f.\n", param, result);
    
    return 0;
}