#include <iostream>
#include <math.h>

using namespace std;

int main(){
    printf("7 ^ 3 = %f\n", pow(7.0, 3.0));
    printf("4.73 ^ 12 = %f\n", pow(4.73, 12.0));
    printf("32.01 ^ 1.54 = %f\n", pow(32.01, 1.54));
    cout << "input your own number: ";
    double a, b;
    cin >> a;
    cin >> b;
    printf("%f ^ %f = %f\n", a, b, pow(a, b));
    return 0;
}