#include <iostream>
#include <string>
using namespace std;

int main(){
    string arr[2][2];
    int row, col, i, j;
    //cout << "Enter number of row for Array (max 10): ";
    //cin >> row;
    //cout << "Enter number of column for Array (max 10): ";
    //cin >> col;
    row = 2;
    col = 2;
    cout << "Now Enter " << row << "*" << col << " Array Elements:\n";
    for(i = 0; i < row; i++){
        for(j = 0; j < col; j++){
            getline(cin, arr[i][j]);
        }
    }
    cout << "\nThe Array is:\n";
    for(i = 0; i < row; i++){
        for(j = 0; j < col; j++){
            cout << arr[i][j] << " ";
        }
        cout << endl;
    }
    
    return 0;
}
